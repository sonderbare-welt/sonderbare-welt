	volume = {15},
	copyright = {All rights reserved},
	issn = {2072-6694},
	shorttitle = {Patient’s {Desire} and {Real} {Availability} {Concerning} {Supportive} {Measures} {Accompanying} {Radical} {Prostatectomy}},
	url = {https://www.mdpi.com/2072-6694/15/10/2830},
	doi = {10.3390/cancers15102830},
	abstract = {Certification as a prostate cancer center requires the offer of several supportive measures to patients undergoing radical prostatectomy (RP). However, it remains unclear how patients estimate the relevance of these measures and whether the availability of these measures differs between certified prostate cancer centers (CERTs) and non-certified centers (NCERTs). In 20 German urologic centers, a survey comprising questions on the relevance of 15 supportive measures was sent to 1000 patients at a median of 15 months after RP. Additionally, patients were asked to rate the availability of these measures using a four-item Likert scale. The aim of this study was to compare these ratings between CERTs and NCERTs. The response rate was 75.0\%. In total, 480 patients underwent surgery in CERTs, and 270 in NCERTs. Patients rated 6/15 supportive measures as very relevant: preoperative medical counselling concerning treatment options, a preoperative briefing answering last questions, preoperative pelvic floor exercises (PFEs), postoperative PFEs, postoperative social support, and postoperative rehabilitation addressing physical fitness recovery. These ratings showed no significant difference between CERTs and NCERTs (p = 0.133–0.676). In addition, 4/9 of the remaining criteria were rated as more detailed by patients in CERTs. IMPROVE represents the first study worldwide to evaluate a patient-reported assessment of the supportive measures accompanying RP. Pertinent offers vary marginally between CERTs and NCERTs.},
	language = {en},
	number = {10},
	urldate = {2023-11-01},
	journal = {Cancers},
	author = {Wolff, Ingmar and Burchardt, Martin and Peter, Julia and Thomas, Christian and Sikic, Danijel and Fiebig, Christian and Promnitz, Sören and Hoschke, Bernd and Burger, Maximilian and Schnabel, Marco J. and Gilfrich, Christian and Löbig, Niklas and Harke, Nina N. and Distler, Florian A. and May, Matthias},
	month = may,
	year = {2023},
	pages = {2830},
