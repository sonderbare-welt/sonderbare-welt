@article{sondermann_nierenzellkarzinom_2025,
	title = {Das {Nierenzellkarzinom}: {Aktuelle} {Therapiealgorithmen}},
	volume = {15},
	issn = {2198-5650, 2568-8251},
	shorttitle = {Das {Nierenzellkarzinom}},
	url = {https://econtent.hogrefe.com/doi/10.1024/2198-5650/a000023},
	doi = {10.1024/2198-5650/a000023},
	abstract = {Die Therapie des Nierenzellkarzinoms (RCC) ist stark stadienabhängig und umfasst chirurgische Ansätze, Systemtherapien und palliative Maßnahmen. Beim lokalisierten RCC steht die kurative operative Entfernung im Fokus, während in metastasierten Stadien eine Kombination aus Immun- und Tyrosinkinase-Inhibitoren sowie individuelle palliative Interventionen erfordern. Die adjuvante Therapie mit Checkpoint-Inhibitoren verbessert die Prognose bei Hochrisikopatienten signifikant. Systemtherapien orientieren sich an Prognosekriterien wie den IMDC-Scores und ermöglichen durch personalisierte Ansätze eine effektive Krankheitskontrolle. Palliative Maßnahmen wie Radiotherapie bei Metastasen oder supportive Therapien spielen eine entscheidende Rolle für die Lebensqualität der Betroffenen. Eine enge interdisziplinäre Zusammenarbeit und die Berücksichtigung individueller Patientenbedürfnisse sind essenziell für die optimale Betreuung.},
	language = {de},
	number = {1},
	urldate = {2025-04-06},
	journal = {Onkologische Pflege},
	author = {Sondermann, Marcus and Chi Le, Quynh and Kloft, Maximilian and Kalogirou, Charis},
	month = mar,
	year = {2025},
	pages = {35--40},
}

@article{menzel_urodynamik_2025,
	title = {Urodynamik in {Deutschland}: {Indikationen} und {Anwendung} im urologischen {Klinik}- und {Praxisalltag}},
	issn = {2731-7064, 2731-7072},
	shorttitle = {Urodynamik in {Deutschland}},
	url = {https://link.springer.com/10.1007/s00120-025-02579-5},
	doi = {10.1007/s00120-025-02579-5},
	abstract = {Zusammenfassung

              Hintergrund
              Urodynamische Studien (UDS) sind ein etabliertes Diagnostikverfahren in der Urologie. Internationale Studien, auch in Deutschland, zeigen jedoch einen Rückgang ihrer Anwendung. Gründe hierfür könnten Leitlinienanpassungen und veränderte Indikationsstellungen sein.


              Fragestellung
              Ziel ist es, die Versorgungsrealität und Indikationsverteilung für UDS in urologischen Kliniken und Praxen in Deutschland zu untersuchen.


              Material und Methoden
              Zwischen 03/2023 und 10/2023 wurden landesweit 259 urologische Kliniken und stichprobenartig 280 niedergelassene Urologen zur UDS befragt. Daten zu angewandten Methoden, durchführendem Personal und Indikationsverteilung wurden erhoben.


              Ergebnisse
              Es nahmen 80 \% der Kliniken und 44 \% der Praxen teil. 58 \% der Kliniken führen {\textless} 100 UDS pro Jahr durch. 15 \% der Praxen führen selbstständig UDS durch. Verfahren wie Uroflowmetrien (99 \%) und Zystomanometrien (98 \%) werden flächendeckend angewandt, während Videourodynamiken (53 \%) seltener zur Verfügung stehen. Häufigste Indikationen in Kliniken sind neurogene Blasenfunktionsstörungen (24 \%), überaktive Harnblasen (21 \%) und Belastungsinkontinenz bei Frauen (19 \%). Praxen zeigen ähnliche Schwerpunkte: neurogene Blasenfunktionsstörungen (30 \%) und überaktive Harnblasen (26 \%). Universitätskliniken und spezialisierte Zentren führen signifikant häufiger UDS bei komplexen Krankheiten durch, während die Versorgung der weiblichen Belastungsinkontinenz häufiger in nichtuniversitären Kliniken erfolgt.


              Schlussfolgerung
              Trotz sinkender Fallzahlen bleiben UDS zentral für die Diagnostik. Die Indikationsverteilung variiert je nach Kliniktyp. Diese Ergebnisse liefern Einblicke in die Praxisanwendung von UDS in Deutschland und unterstreichen ihre Rolle in der urologischen Diagnostik.


